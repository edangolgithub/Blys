    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Attach to Edge",
            "port": 8081,
            "request": "attach",
            "type": "pwa-msedge",
            "webRoot": "${workspaceFolder}"
        },

        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceFolder}"
        }
    ]
}